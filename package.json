{
  "name": "notes-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon src/index.ts --files ./global.d.ts",
    "dev": "concurrently  \"nodemon -x tsoa spec\" \"nodemon src/index.ts --files ./global.d.ts\"",
    "build-docs": "tsoa spec",
    "pretest": "mongo db --eval \"db.dropDatabase();\"",
    "test": "jest --testTimeout=10000 --detectOpenHandles",
    "coverage": "jest --coverage"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*.json"
    ]
  },
  "dependencies": {
    "@types/validator": "^13.1.3",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.10.0",
    "mongoose": "^5.12.3",
    "morgan": "~1.9.1",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.6.1",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.39",
    "@types/passport": "^1.0.6",
    "@types/passport-http": "^0.3.8",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^6.0.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "utility-types": "^3.10.0"
  }
}
