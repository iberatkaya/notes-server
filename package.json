{
  "name": "notes-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node build/src/index.js",
    "dev": "concurrently  \"nodemon -x tsoa spec\" \"nodemon src/index.ts --files ./global.d.ts\"",
    "build-docs": "tsoa spec",
    "pretest": "mongo db --eval \"db.dropDatabase();\"",
    "test": "jest --testTimeout=10000 --detectOpenHandles",
    "coverage": "jest --coverage",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "dist": "sh ./scripts/dist.sh",
    "deploy": "npm run dist && eb deploy --staged"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*.json"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.10.0",
    "mongoose": "^5.12.3",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.22",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.6.1",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.39",
    "@types/nodemailer": "^6.4.1",
    "@types/passport": "^1.0.6",
    "@types/passport-http": "^0.3.8",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4",
    "utility-types": "^3.10.0"
  }
}
